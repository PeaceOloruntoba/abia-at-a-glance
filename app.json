{
  "expo": {
    "name": "abia-at-a-glance",
    "slug": "abia-at-a-glance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.peaceoloruntoba.abiaataglance",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-video",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "d0ce7ad2-92a1-4579-aea8-06b276aedcf9"
      }
    }
  }
}
